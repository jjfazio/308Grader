<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="350.0000847412084" prefWidth="384.99988474121346" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="controller.spreadsheet.AddGradingSchemeController">
  <!-- TODO Add Nodes -->
  <children>
    <VBox alignment="CENTER" layoutX="0.0" layoutY="0.0" prefHeight="333.0" prefWidth="385.0">
      <children>
        <TableView fx:id="rangesTable" editable="true" prefHeight="166.0" prefWidth="385.0" tableMenuButtonVisible="false">
          <columns>
            <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="108.0" text="Symbol" fx:id="colSymbols" />
            <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="117.0" text="Low %" fx:id="colLowPercent" />
            <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="117.0" text="High %" fx:id="colHighPercent" />
          </columns>
        </TableView>
        <HBox alignment="TOP_CENTER" prefHeight="32.0" prefWidth="378.0">
          <children>
            <TextField fx:id="newSymbol" prefWidth="200.0" promptText="A" />
            <TextField fx:id="newLowPercent" prefWidth="200.0" promptText="90.0" />
            <TextField fx:id="newHighPercent" prefWidth="200.0" promptText="100.0" />
            <Button mnemonicParsing="false" onAction="#handlePlusButton" text="+" />
          </children>
        </HBox>
        <HBox alignment="CENTER" prefHeight="51.0" prefWidth="385.0">
          <children>
            <Label text="Scheme Name:" />
            <TextField fx:id="schemeName" prefWidth="200.0" />
          </children>
        </HBox>
        <HBox alignment="CENTER" prefHeight="44.0" prefWidth="385.0">
          <children>
            <Button mnemonicParsing="false" onAction="#handleCreateButton" text="Create" />
            <Button mnemonicParsing="false" onAction="#handleCancelButton" text="Cancel" />
          </children>
        </HBox>
      </children>
    </VBox>
  </children>
</AnchorPane>
